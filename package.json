  "contributors": [
    "Juan Carlos Farah",
    "Teele JÃ¼rivete",
    "Kim Lan Phan Hoang"
  ],
  "homepage": ".",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.2",
    "@fortawesome/free-solid-svg-icons": "6.1.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@graasp/apps-query-client": "github:graasp/graasp-apps-query-client.git#35/passthrough",
    "@graasp/ui": "github:graasp/graasp-ui.git",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@sketchfab/viewer-api": "1.12.1",
    "clsx": "1.2.1",
    "i18next": "21.9.1",
    "immutable": "4.1.0",
    "lodash.sortby": "4.7.0",
    "prop-types": "15.8.1",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga": "3.3.1",
    "react-i18next": "11.18.4",
    "react-iframe": "1.8.0",
    "react-qr-svg": "2.4.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "9.0.8"
  },
  "scripts": {
    "start": "env-cmd -f .env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts -r @cypress/instrument-cra start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override ./.env.prod yarn build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.js'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.js'",
    "test": "react-scripts test",
    "test:once": "cross-env CI=true env-cmd ./.env.test react-scripts test --env=jsdom",
    "test:coverage": "cross-env CI=true env-cmd ./.env.test react-scripts test --env=jsdom --coverage",
    "test:ci": "run-s test:coverage report",
    "report": "cat ./coverage/lcov.info | env-cmd ./.env.test codacy-coverage",
    "eject": "react-scripts eject",
    "release": "git add CHANGELOG.md && standard-version -a",
    "pre-commit": "yarn prettier:check && yarn lint",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/**/*.cy.js\""
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.9",
    "@babel/eslint-plugin": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "cross-env": "7.0.3",
    "cypress": "10.6.0",
    "env-cmd": "10.1.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "husky": "8.0.1",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "standard-version": "9.5.0"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/serviceWorker.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.2.1"
}
