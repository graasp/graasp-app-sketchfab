    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.2.1",
    "cross-env": "7.0.3",
    "cypress": "13.6.3",
    "env-cmd": "10.1.0",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
