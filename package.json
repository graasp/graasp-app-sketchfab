    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "nyc": "15.1.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-istanbul": "5.0.0"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/serviceWorker.js"
